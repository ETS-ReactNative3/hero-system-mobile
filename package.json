{
  "name": "hero-system-mobile",
  "version": "1.7.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.6",
    "@react-native-community/eslint-plugin": "^1.0.0",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "husky": "^4.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.8.6",
    "rn-nodeify": "^10.2.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "linter": "eslint",
    "postinstall": "rn-nodeify --install stream,process,util,buffer --hack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "(?!shim)*{.js,test.js}": [
      "npm run linter --",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-navigation-drawer|@react-navigation|native-base|@react-native-community/async-storage)"
    ]
  },
  "browser": {
    "net": "react-native-tcp",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "dependencies": {
    "@bankify/react-native-animate-number": "^0.2.1",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.5",
    "babel-plugin-react-native-nodeify-hack": "^1.0.1",
    "buffer": "^4.9.2",
    "camelcase": "^5.3.1",
    "faker": "^4.1.0",
    "iconv-lite": "^0.5.1",
    "is-base64": "^1.0.0",
    "native-base": "2.13.8",
    "process": "^0.11.10",
    "react": "16.8.6",
    "react-native": "0.60.6",
    "react-native-calculator": "^0.5.2",
    "react-native-document-picker": "^3.2.4",
    "react-native-dropdown-autocomplete": "^1.0.18",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-modal": "^11.5.3",
    "react-native-network-info": "^5.2.1",
    "react-native-pathjs-charts": "0.0.34",
    "react-native-pie-chart": "^1.0.13",
    "react-native-pulse": "^1.0.7",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-shake": "^3.4.0",
    "react-native-size-matters": "^0.3.0",
    "react-native-slider": "^0.11.0",
    "react-native-sound": "^0.11.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.13.6",
    "react-native-svg-charts": "^3.0.2",
    "react-native-tcp": "^3.3.2",
    "react-native-xml2js": "^1.0.3",
    "react-native-zip-archive": "^5.0.0",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.3",
    "react-redux": "^7.1.3",
    "readable-stream": "^1.0.33",
    "redux": "^4.0.4",
    "redux-enhancer-react-native-appstate": "^0.3.1",
    "redux-mock-store": "1.5.1",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "^0.12.0",
    "snake-case": "^2.1.0",
    "stream": "0.0.2",
    "stream-browserify": "^1.0.0"
